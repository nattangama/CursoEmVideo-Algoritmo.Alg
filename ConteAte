algoritmo "ConteAte"
var
   Valor, Salto, Contador : Inteiro
inicio
      Contador <- 0
      Escreva("Quer contar ate quanto? ")
      Leia(Valor)
      Escreva("Qual o valor do salto? ")
      Leia(Salto)
      Enquanto (contador <= valor )faca
           EscrevaL(contador)
           contador <- contador + 1
      FimEnquanto
      EscrevaL("Encerrou a contagem!")
fimalgoritmo
